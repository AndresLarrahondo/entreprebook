<div class="product-page">
  <header class="product-header">
    <h1><%= @book.name %></h1>
    <p><%= @book.description %></p>
    <a href="<%= @book.url%>" target="_blank"><span class="label  label-primary">Consigelo Ya</span></a>
  </header>

  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <div class="votes">
          <h2>Votos</h2>
        </div>
        <div class="comments">
          <h2>Comentarios</h2>
        </div>
      </div>
    </div>
  </div>
</div>
